{
  "name": "link-grabber",
  "version": "2.0.1",
  "description": "Suggest links while you type. Based on Obsidian's native search and supercharged with AI semantic recognition.",
  "main": "main.ts",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "auto-complete",
    "search",
    "ai",
    "semantic"
  ],
  "author": "IxxSxxA",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.25.0",
    "esbuild-plugin-inline-worker": "^0.1.1",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "obsidian": "latest",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.8.0"
  }
}